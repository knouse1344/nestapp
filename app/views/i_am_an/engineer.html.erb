<script>
// Using multiple unit types within one animation.
var $number = 0;

$( ".next_btn" ).click(function() {
	if($number < 4) {
	  $( ".digital" ).animate({
	    left: "-=100%"
	  }, 1200 );
	  $( ".concepts" ).animate({
	    left: "-=100%"
	  }, 1200 );
	  $(".design").animate({
	  	left: "-=100%"
	  }, 1200 );
	  $(".testing").animate({
	  	left: "-=100%"
	  }, 1200 );
	  $(".construction").animate({
	  	left: "-=100%"
	  }, 1200 );
	  $number += 1;
	} else {
	};
	if($number == 4) {
		$('.next_btn').css('opacity', '0');
		$('.next_btn').css('cursor', 'auto');
	}
	if($number == 1) {
		$('.prev_btn').css('opacity', '1');
		$('.prev_btn').css('cursor', 'pointer');
	}
});

$( ".prev_btn" ).click(function() {
	if($number > 0) {
	  $( ".digital" ).animate({
	    left: "+=100%"
	  }, 1200 );
	  $( ".concepts" ).animate({
	    left: "+=100%"
	  }, 1200 );
	  $(".design").animate({
	  	left: "+=100%"
	  }, 1200 );
	  $(".testing").animate({
	  	left: "+=100%"
	  }, 1200 );
	  $(".construction").animate({
	  	left: "+=100%"
	  }, 1200 );
	  $number -= 1;
	} else {
	};
	if($number == 3) {
		$('.next_btn').css('opacity', '1');
		$('.next_btn').css('cursor', 'pointer');
	};
	if($number == 0) {
		$('.prev_btn').css('opacity', '0');
		$('.prev_btn').css('cursor', 'auto');
	}
});
</script>